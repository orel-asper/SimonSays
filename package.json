{
  "name": "simonsays",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "restart": "npx react-native-clean-project",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "assets": "cd ./src/assets/svg && node reg_svg.js && cd .. && cd png && node reg_img.js && cd .. && cd lottie && node reg_lottie.js",
    "build-test-android": "cd ./src/android && ./gradlew assembleDebug",
    "build-apk": "npx react-native run-android --variant=release",
    "android-release": "cd android && ./gradlew assembleRelease",
    "android-bundle-release": "cd android && ./gradlew bundleRelease",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-navigation/bottom-tabs": "^6.3.3",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.8.0",
    "lottie-react-native": "^5.1.4",
    "react": "18.0.0",
    "react-native": "0.69.5",
    "react-native-fast-image": "^8.6.0",
    "react-native-gesture-handler": "^2.6.0",
    "react-native-reanimated": "^2.10.0",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-screens": "^3.17.0",
    "react-native-svg": "^13.0.0",
    "react-native-system-navigation-bar": "^2.0.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.69.5",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
